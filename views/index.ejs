                <!-- Animations for Service viewport -->

                <script>
                    document.addEventListener('DOMContentLoaded', function () {
                        gsap.registerPlugin(ScrollTrigger);
                
                        
                        function animateServices() {
                            gsap.utils.toArray('.single-service').forEach((service, index) => {
                                gsap.from(service, {
                                    opacity: 0,
                                    y: 50,
                                    duration: 1,
                                    ease: 'power2.out',
                                    scrollTrigger: {
                                        trigger: service,
                                        start: 'top 80%',
                                        toggleActions: 'play none none reverse',
                                    },
                                });
                
                                
                                service.addEventListener('mouseenter', () => {
                                    gsap.to(service, {
                                        scale: 1.05,
                                        boxShadow: '0 10px 20px rgba(0,0,0,0.2)',
                                        duration: 0.3,
                                        ease: 'power2.out',
                                    });
                                });
                
                                service.addEventListener('mouseleave', () => {
                                    gsap.to(service, {
                                        scale: 1,
                                        boxShadow: '0 4px 6px rgba(0,0,0,0.1)',
                                        duration: 0.3,
                                        ease: 'power2.out',
                                    });
                                });
                            });
                        }
                
                        
                        gsap.from('.section-heading', {
                            opacity: 0,
                            y: -50,
                            duration: 1.5,
                            ease: 'power2.out',
                            scrollTrigger: {
                                trigger: '.section-heading',
                                start: 'top 80%',
                            },
                        });
                
                       
                        gsap.from('.single-service', {
                            opacity: 0,
                            y: 50,
                            stagger: 0.2,
                            duration: 1,
                            ease: 'power2.out',
                            scrollTrigger: {
                                trigger: '.single-service',
                                start: 'top 90%',
                                toggleActions: 'play none none none',
                            },
                            onComplete: animateServices, 
                        });
                    });
                </script>

