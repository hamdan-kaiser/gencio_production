                                        }
                                    );

                                    service.addEventListener(
                                        "mouseleave",
                                        () => {
                                            gsap.to(service, {
                                                scale: 1,
                                                boxShadow:
                                                    "0 4px 6px rgba(0,0,0,0.1)",
                                                duration: 0.3,
                                                ease: "power2.out",
