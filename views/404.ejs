<!doctype html>
<html class="no-js" lang="en">
<%- include('src/header.ejs') %>
<body class="error-page">
    <%- include('src/preloader.ejs') %>

    <!--====== 404 Area Start ======-->
    <div class="error-area stars">
        <!-- Error Content -->
        <div class="error-content d-flex flex-column justify-content-center align-items-center text-center">
            <img class="error-thumb" src="/img/error/404.svg">
            <a href="index.html" class="btn btn-bordered-white mt-4">GO BACK HOME</a>
        </div>
        <!-- Error Objects -->
        <div class="error-objects">
            <img class="thumb-1" src="/img/error/rocket.svg">
            <!-- Earth Moon -->
            <div class="earth-moon">
                <img class="thumb-2" src="/img/error/earth.svg">
                <img class="thumb-3" src="/img/error/moon.svg">
            </div>
            <!-- Astronaut -->
            <div class="astronaut">
                <img class="thumb-4" src="/img/error/astronaut.svg">
            </div>
        </div>
        <!-- Glowing Stars -->
        <div class="glowing-stars">
            <div class="star"></div>
            <div class="star"></div>
            <div class="star"></div>
            <div class="star"></div>
            <div class="star"></div>
        </div>
    </div>
    <!--====== 404 Area End ======-->


    <!-- ***** All jQuery Plugins ***** -->

    <!-- jQuery(necessary for all JavaScript plugins) -->
    <script src="/js/jquery/jquery-3.5.1.min.js"></script>

    <!-- Bootstrap js -->
    <script src="/js/bootstrap/popper.min.js"></script>
    <script src="/js/bootstrap/bootstrap.min.js"></script>

    <!-- Plugins js -->
    <script src="/js/plugins/plugins.min.js"></script>

    <!-- Active js -->
    <script src="/js/active.js"></script>
</body>

</html>